<div class="author">
    <h1 class="author__heading">Search Results</h1>
</div>

<section class="articles">
    <ul class="article-ul">
        <% data.forEach(post=> { %>
            <li>
                <a href="/post/<%= post._id %>">
                    <span>
                        <%= post.title %>
                            <!-- Post Title -->
                    </span>
                    <span class="article-list__date">
                        <%= post.createdAt.toDateString() %>
                            <!-- Date -->
                    </span>
                </a>
            </li>
        <% }) %>
    </ul>
</section>